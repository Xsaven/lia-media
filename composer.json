{
  "name": "xsaven/lia-media",
  "description": "MediaController extension for LIA",
  "type": "library",
  "keywords": ["lia", "media"],
  "homepage": "https://github.com/xsacen/lia-media",
  "license": "MIT",
  "authors": [
    {
      "name": "xsaven",
      "email": "gms.sw.xsaven@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "5.*",
    "xsaven/laravel-intelect-admin": "*",
    "unisharp/laravel-filemanager": "^1.8",
    "intervention/image": "~2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "laravel/laravel": "5.*"
  },
  "autoload": {
    "psr-4": {
      "Lia\\Media\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lia\\Media\\MediaServiceProvider"
      ]
    }
  }
}